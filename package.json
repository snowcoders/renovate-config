{
  "author": "k2snowman69",
  "dependencies": {
    "exec-if-exists": "^3.0.0"
  },
  "description": "Renovate and other configs for all Snowcoders projects to inherit from",
  "devDependencies": {
    "@snowcoders/sortier": "3.3.0",
    "@types/jest": "29.2.3",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "eslint": "8.27.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "ts-jest": "29.0.3",
    "tslib": "2.4.1",
    "typescript": "4.8.4"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "name": "@snowcoders/renovate-config",
  "renovate-config": {
    "default": {
      "automerge": true,
      "azureAutoComplete": true,
      "extends": [
        "group:monorepos",
        "group:recommended"
      ]
    }
  },
  "repository": "github:snowcoders/renovate-config",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf coverage dist",
    "husky:commit-msg": "echo 'No commit message hook'",
    "husky:pre-commit": "npx --no lint-staged",
    "husky:pre-push": "npm test",
    "prepare": "npm run clean && npm run build && npx --no husky install",
    "test": "jest"
  },
  "tsconfig": "./tsconfig.base.json",
  "version": "2.0.3"
}
