    "declaration": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "target": "es5"
  }
}
